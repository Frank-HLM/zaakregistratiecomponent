<ZDS:voegBesluitToe_Di01 xsi:schemaLocation="http://www.stufstandaarden.nl/koppelvlak/zds0120 ../zds0120_msg_zs-dms.xsd"
                         xmlns:StUF="http://www.egem.nl/StUF/StUF0301"
                         xmlns:ZKN="http://www.egem.nl/StUF/sector/zkn/0310"
                         xmlns:ZDS="http://www.stufstandaarden.nl/koppelvlak/zds0120"
                         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ZKN:stuurgegevens>
        <StUF:berichtcode>Di01</StUF:berichtcode>
        <StUF:zender>
            <StUF:organisatie>{{ zenderOrganisatie }}</StUF:organisatie>
            <StUF:applicatie>{{ zenderApplicatie }}</StUF:applicatie>
            <StUF:gebruiker/>
        </StUF:zender>
        <StUF:ontvanger>
            <StUF:organisatie>{{ ontvangerOrganisatie }}</StUF:organisatie>
            <StUF:applicatie>{{ ontvangerApplicatie }}</StUF:applicatie>
            <StUF:gebruiker/>
        </StUF:ontvanger>
        <StUF:referentienummer>{{ referentienummer }}</StUF:referentienummer>
        <StUF:tijdstipBericht>{{ tijdstipBericht }}</StUF:tijdstipBericht>
        <StUF:functie>voegBesluitToe</StUF:functie>
    </ZKN:stuurgegevens>
    <ZKN:object>
        <ZKN:besluit StUF:verwerkingssoort="T" StUF:entiteittype="BSL">
            <ZKN:identificatie>{{ voegbesluittoe_identificatie_3 }}</ZKN:identificatie>
            <ZKN:bst.omschrijving>omschrijving</ZKN:bst.omschrijving>
            <ZKN:datumBeslissing>{{ datumVandaag }}</ZKN:datumBeslissing>
            <ZKN:toelichting>toelichting</ZKN:toelichting>
            <ZKN:ingangsdatumWerking>{{ datumVandaag }}</ZKN:ingangsdatumWerking>
            <ZKN:einddatumWerking>{{ datumVandaag }}</ZKN:einddatumWerking>
            <ZKN:vervalreden>Besluit met tijdelijke werking</ZKN:vervalreden>
            <ZKN:datumPublicatie>{{ datumVandaag }}</ZKN:datumPublicatie>
            <ZKN:datumVerzending>{{ datumVandaag }}</ZKN:datumVerzending>
            <ZKN:datumUiterlijkeReactie>{{ datumVandaag }}</ZKN:datumUiterlijkeReactie>
            <StUF:tijdvakGeldigheid>
                <StUF:beginGeldigheid>{{ datumEergisteren }}</StUF:beginGeldigheid>
                <StUF:eindGeldigheid StUF:noValue="geenWaarde" xsi:nil="true"/>
            </StUF:tijdvakGeldigheid>
            <!--
            <StUF:tijdstipRegistratie>{{ tijdstipRegistratie }}</StUF:tijdstipRegistratie>
            -->
            <ZKN:isVastgelegdIn StUF:verwerkingssoort="T" StUF:entiteittype="BSLEDC">
                <ZKN:gerelateerde StUF:verwerkingssoort="I" StUF:entiteittype="EDC">
                    <ZKN:identificatie>{{ voegzaakdocumenttoe_identificatie_1 }}</ZKN:identificatie>
                    <ZKN:dct.omschrijving>omschrijving</ZKN:dct.omschrijving>
                    <ZKN:titel>titel</ZKN:titel>
                </ZKN:gerelateerde>
            </ZKN:isVastgelegdIn>
            <ZKN:isVastgelegdIn StUF:verwerkingssoort="T" StUF:entiteittype="BSLEDC">
                <ZKN:gerelateerde StUF:verwerkingssoort="I" StUF:entiteittype="EDC">
                    <ZKN:identificatie>{{ voegzaakdocumenttoe_identificatie_3 }}</ZKN:identificatie>
                    <ZKN:dct.omschrijving>omschrijving</ZKN:dct.omschrijving>
                    <ZKN:titel>titel</ZKN:titel>
                </ZKN:gerelateerde>
            </ZKN:isVastgelegdIn>
        </ZKN:besluit>
        <ZKN:zaak StUF:verwerkingssoort="T" StUF:entiteittype="ZAK">
            <ZKN:identificatie>{{ gemeentecode }}{{ creerzaak_identificatie_7 }}</ZKN:identificatie>
        </ZKN:zaak>
    </ZKN:object>
</ZDS:voegBesluitToe_Di01>
